NEW LINK
<br>
==========
<br><br>
<%= form_for @link, :url => { :action => "create" } do |f| %>
  <%= f.label :url %>
  <%= f.text_field :url %>
  <br>  
  <%= f.submit "Create" %>
<% end %>

<br><br><br><br>

NEW VENUE
<br>
==========
<br><br>
<%= form_for @venue, :url => { :action => "create" } do |f| %>
  <%= f.fields_for :item do |i| %>
    <%= i.label :name %>
    <%= i.text_field :name %>
    <br>
  <% end %>
  <%= f.label :address %>
  <%= f.text_field :address %>
  <br>  
  <%= f.submit "Create" %>
<% end %>
